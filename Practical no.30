import java.util.HashMap;
import java.util.Scanner;

public class StudentHashMap {
  public static void main(String[] args) {
    HashMap<Integer, String> students = new HashMap<>();
    students.put(101, "parmar nilesh");
    students.put(102, "ambaliya mahesh");
    students.put(103, "ram ariyan");
    students.put(104, "parmar manav");
    students.put(105, "parmar karan");

    Scanner scanner = new Scanner(System.in);
    System.out.print("Enter enrolment number: ");
    int enrolmentNo = scanner.nextInt();
    String studentName = students.get(enrolmentNo);

    if (studentName != null) {
      System.out.println("Student Name: " + studentName);
    } else {
      System.out.println("No student found with enrolment number: " + enrolmentNo);
    }
    scanner.close();
  }
}
